<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.frogs.matzip.IndexMapper">  
	<insert id="insUser" parameterType="IndexVO">
		INSERT INTO t_user
			(user_id, user_pw, salt, nm)
			VALUES
			(#{user_id}, #{user_pw}, #{salt}, #{nm})
	</insert>
</mapper>